{
  "name": "spicecraft.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "prod-build": "ng build --configuration production --aot --base-href /app/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "devextreme": "^24.1.4",
    "devextreme-angular": "^24.1.4",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "file-saver-es": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jest-editor-support": "*",
    "jspdf": "^2.5.2",
    "ngx-parallax": "^4.0.0",
    "rellax": "^1.12.1",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "swiper": "^11.1.9",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@types/aos": "^3.0.7",
    "@types/file-saver": "^2.0.7",
    "@types/file-saver-es": "^2.0.3",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}
